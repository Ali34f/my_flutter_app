{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "quotes": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "valid-jsdoc": "off"
  },
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  }
}
